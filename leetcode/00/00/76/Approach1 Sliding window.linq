// 76. Minimum Window Substring
// https://leetcode.com/problems/minimum-window-substring/

/*
    Time: O(N + M) where N and M represent the lengths of strings S and T. In the worst case we might end up visiting every element of string S twice, once by left pointer and once by right pointer.
    Space: O(N + M) N when the window size is equal to the entire string S. M when T has all unique characters.
*/
public class Solution
{
    public string MinWindow(string s, string t)
    {
        if (s.Length == 0 || t.Length == 0)
            return string.Empty;

        var dictT = new Dictionary<char, int>();
        for (int i = 0; i < t.Length; i++)
        {
            if(!dictT.ContainsKey(t[i]))
                dictT.Add(t[i], 0);
            dictT[t[i]]++;
        }

        var required = dictT.Count;

        var l = 0;
        var r = 0;

        var formed = 0;

        var windowCounts = new Dictionary<char, int>();

        var answer = new int[] { -1, 0, 0 };

        while (r < s.Length)
        {
            var c = s[r];
            if(!windowCounts.ContainsKey(c))
                windowCounts.Add(c, 0);
            windowCounts[c]++;

            if (dictT.ContainsKey(c) && windowCounts[c] == dictT[c])
                formed++;

            while (l <= r && formed == required)
            {
                c = s[l];
                if (answer[0] == -1 || r - l + 1 < answer[0])
                {
                    answer[0] = r - l + 1;
                    answer[1] = l;
                    answer[2] = r;
                }

                windowCounts[c]--;
                if (dictT.ContainsKey(c) && windowCounts[c] < dictT[c])
                    formed--;

                l++;
            }

            r++;   
        }

        return answer[0] == -1 ? string.Empty : s.Substring(answer[1], answer[2] - answer[1] + 1);
    }
}

[Test]
[TestCase("ADOBECODEBANC", "ABC", "BANC")]
[TestCase("a", "a", "a")]
[TestCase("a", "aa", "")]
[TestCase("",
"", "")]
public void SolutionTests(string s, string t, string expected)
{
    var actual = new Solution().MinWindow(s, t);
    Assert.That(actual, Is.EqualTo(expected));
}

#region unit tests runner

void Main()
{
    var workDir = Path.Combine(Util.MyQueriesFolder, "nunit-work");

    var args = new string[]
    {
         "-noheader",
         $"--work={workDir}",
    };

    RunUnitTests(args);
}

void RunUnitTests(string[] args, Assembly assembly = null)
{
    Console.SetOut(new NoDisposeTextWriter(Console.Out));
    Console.SetError(new NoDisposeTextWriter(Console.Error));
    new AutoRun(assembly ?? Assembly.GetExecutingAssembly()).Execute(args);
}

// https://stackoverflow.com/q/52883672/5752652
class NoDisposeTextWriter : TextWriter
{
    private readonly TextWriter writer;
    public NoDisposeTextWriter(TextWriter writer) => this.writer = writer;

    public override Encoding Encoding => writer.Encoding;
    public override IFormatProvider FormatProvider => writer.FormatProvider;
    public override void Write(char value) => writer.Write(value);
    public override void Flush() => writer.Flush();
    // forward all other overrides as necessary

    protected override void Dispose(bool disposing)
    {
        // no nothing
    }
}

#endregion