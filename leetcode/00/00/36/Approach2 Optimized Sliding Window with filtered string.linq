// 76. Minimum Window Substring
// https://leetcode.com/problems/minimum-window-substring/

/*
    Time: O(N + M) The complexity is same as the previous approach. But in certain cases where filtered_S <<< S, the complexity would reduce because the number of iterations would be 2 * filtered_S.Length + N + M
    Space: O(N + M)
*/
public class Solution
{
    public string MinWindow(string s, string t)
    {
        if (s.Length == 0 || t.Length == 0)
            return string.Empty;

        var dictT = new Dictionary<char, int>();
        for (int i = 0; i < t.Length; i++)
        {
            if(!dictT.ContainsKey(t[i]))
                dictT.Add(t[i], 0);
            dictT[t[i]]++;
        }

        var required = dictT.Count;

        var filteredS = new List<KeyValuePair<int, char>>();
        for (int i = 0; i < s.Length; i++)
        {
            var c = s[i];
            if (dictT.ContainsKey(c))
                filteredS.Add(new KeyValuePair<int, char>(i, c));
        }

        var l = 0;
        var r = 0;
        var formed = 0;
        
        var windowCounts = new Dictionary<char, int>();
        var answer = new int[] { -1, 0, 0 };

        while (r < filteredS.Count)
        {
            var c = filteredS[r].Value;
            if(!windowCounts.ContainsKey(c))
                windowCounts.Add(c, 0);
            windowCounts[c]++;

            if (dictT.ContainsKey(c) && windowCounts[c] == dictT[c])
                formed++;

            while (l <= r && formed == required)
            {
                c = filteredS[l].Value;

                var end = filteredS[r].Key;
                var start = filteredS[l].Key;
                if (answer[0] == -1 || end - start + 1 < answer[0])
                {
                    answer[0] = end - start + 1;
                    answer[1] = start;
                    answer[2] = end;
                }

                windowCounts[c]--;
                if (dictT.ContainsKey(c) && windowCounts[c] < dictT[c])
                    formed--;
                l++;
            }
            r++;   
        }

        return answer[0] == -1 ? string.Empty : s.Substring(answer[1], answer[2] - answer[1] + 1);
    }
}

[Test]
[TestCase("ADOBECODEBANC", "ABC", "BANC")]
[TestCase("a", "a", "a")]
[TestCase("a", "aa", "")]
[TestCase("",
"", "")]
public void SolutionTests(string s, string t, string expected)
{
    var actual = new Solution().MinWindow(s, t);
    Assert.That(actual, Is.EqualTo(expected));
}

#region unit tests runner

void Main()
{
    var workDir = Path.Combine(Util.MyQueriesFolder, "nunit-work");

    var args = new string[]
    {
         "-noheader",
         $"--work={workDir}",
    };

